{
    "name": "m2de/bugherd-laravel",
    "description": "A Laravel Helper package to install the [Bugherd](https://www.bugherd.com/) script",
    "type": "package",
    "keywords": [
        "laravel",
        "bugherd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": ">=5.6.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "M2de\\Bugherd\\Laravel\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "M2de\\Bugherd\\Laravel\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
